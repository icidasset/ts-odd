{
  "name": "@oddjs/odd",
  "version": "0.40.0-prerelease",
  "description": "ODD SDK",
  "keywords": [
    "WebCrypto",
    "auth",
    "files",
    "distributed",
    "DAG",
    "DID",
    "IPFS",
    "IPLD",
    "UCAN",
    "WNFS"
  ],
  "type": "module",
  "main": "lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./lib/*": ["./lib/*.js", "./lib/*", "./lib/*/index.js"],
    "./*": ["./lib/*.js", "./lib/*", "./lib/*/index.js", "./*"]
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "*": {
      "lib/index.d.ts": ["lib/index.d.ts"],
      "*": ["lib/*"]
    }
  },
  "files": [
    "lib",
    "dist",
    "docs",
    "src",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "package.json",
    "!*.test.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oddsdk/ts-odd"
  },
  "homepage": "https://odd.dev",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "docs": "rimraf docs && typedoc",
    "lint": "eslint src/**/*.ts src/*.ts tests/**/*.ts tests/*.ts",
    "prebuild": "rimraf lib dist && node scripts/gen-version.js",
    "build": "tsc && npm run build:minified",
    "build:minified": "node scripts/build-minified.js",
    "format": "dprint fmt",
    "start": "tsc -w",
    "test": "mocha",
    "test:gh-action": "TEST_ENV=gh-action npm run test",
    "test:imports": "madge src --ts-config tsconfig.json --extensions ts --circular --warning",
    "test:prod": "npm run build && npm run lint && npm run test:imports && npm run test:gh-action",
    "test:unit": "mocha --watch --testPathPattern=src/",
    "test:wasm": "WNFS_WASM=true mocha",
    "prepare": "npm run build && npm run docs",
    "publish-dry": "npm publish --dry-run",
    "publish-alpha": "npm publish --tag alpha",
    "publish-latest": "npm publish --tag latest"
  },
  "dependencies": {
    "@ipld/dag-cbor": "^9.2.0",
    "@ipld/dag-pb": "^4.0.6",
    "@ucans/core": "^0.12.0",
    "@wnfs-wg/nest": "^0.3.2",
    "emittery": "^1.0.3",
    "interface-blockstore": "^5.0.0",
    "iso-base": "^2.0.1",
    "iso-did": "^1.6.0",
    "iso-signatures": "^0.1.10",
    "it-all": "^3.0.4",
    "multiformats": "^13.1.0",
    "uint8arrays": "^5.0.0"
  },
  "peerDependencies": {
    "@localfirst/relay": "^4.2.2",
    "@noble/ciphers": "^0.5.1",
    "@noble/curves": "^1.3.0",
    "@noble/hashes": "^1.3.3",
    "blockstore-idb": "^1.1.8",
    "localforage": "^1.10.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@ipld/car": "^5.2.6",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "assert": "^2.1.0",
    "copyfiles": "^2.4.1",
    "dprint": "^0.45.0",
    "esbuild": "^0.20.1",
    "esbuild-plugin-wasm": "^1.1.0",
    "eslint": "^8.49.0",
    "fast-check": "^3.15.1",
    "globby": "^14.0.1",
    "madge": "^6.1.0",
    "mocha": "^10.3.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.8",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.3.3",
    "util": "^0.12.4"
  }
}
